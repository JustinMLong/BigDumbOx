/* --------------- Body Properties --------------- */
html 
	height: 100%;


body 
	font-family: "Trebuchet MS", "Lucida Sans Unicode", "Lucida Grande", "Lucida Sans", Arial, sans-serif;
	text-align: left;
	font-size: 10pt;
	color: white;
	background-color: gray;
	margin:0 auto;
	text-align: left;


header 
	width:100%;
	background-color: white;
	height:150px;
	overflow:visible;
	position: fixed;
	padding-right: 10%;
	z-index: 3;
	text-align: center;


header a 
	text-decoration:none;


header img 
	float: right;
	position:relative;
	padding-right:10%;
	height: 150px;
	z-index: 3;
	border:none;


navigation 
	font-size:12px;
	background-color: white;
	width:100%;
	position: absolute; 
	left: 0px; 
	bottom: 30px;
	padding-left: 0;
	padding-right: 10%;
	text-align:left;
	font-style: italic;


div#content 
	position: absolute; 
	top: 150px;
	padding-left:10%;
	padding-right:10%; 
	z-index: 0;
	width: 80%;


navigation ul 
	margin:0px; 
	padding-left: 10%;
	z-index: 3;


navigation ul li 
	line-height:25px;
	float: left;
	max-height:25px;
	list-style:none;
	cursor:pointer; 
	text-align:left;
	font-size: 12pt;
	min-width: 2em;
	margin: 0 15px 0 15px;


navigation ul li:hover a:before 
	content: "[ "; 
	color: maroon; 
	position: absolute; 
	left: -.75em;
navigation ul li:hover a:after	
	content: " ]"; 
	color: maroon; 
	position: absolute; 
	right: -.75em;

navigation ul li a 
	text-decoration:none;
	display:block;
	color: gray;


navigation ul li a:visited 
	text-decoration:none;
	display:block;
	color: gray;


navigation ul li:hover 
	line-height:25px;
	min-height:25px;
	position:relative;


#oxlogo 
	position: absolute; top: 17px; left: 5px; 


